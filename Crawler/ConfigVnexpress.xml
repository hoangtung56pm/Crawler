    <CategoryID>1</CategoryID>
    <HttpPrefix>http://vnexpress.net</HttpPrefix>
    <ListStartAfter>
      <![CDATA[<div class="folder-top">.*]]>
    </ListStartAfter>
    <ListEndAt>
      <![CDATA[<script type="text/javascript" language="javascript">.*]]>
    </ListEndAt>
    <ParternList>
      <![CDATA[<div class="folder-news">.*?<div class="left-fnews">.*?<a class="aImg130" href=".*?">.*?<img src="(?<ImagePath>[^"<]+)?" alt=".*?" class="img-subject">.*?</a>.*?</div>.*?<div class="right-fnews">.*?<h2 class="h2Title-14">.*?<a class="link-title14" href="(?<Link>[^"<]+)?">(?<Title>.*?)</a>.*?</h2>.*?<h3 class="h3Lead">(?<Teaser>.*?)</h3>.*?</div>.*?</div>]]>
    </ParternList>
    <DetailStartAfter>
      <![CDATA[<div id="content".*?>.*]]>
    </DetailStartAfter>
    <DetailEndAt>
      <![CDATA[<div id="footer">.*]]>
    </DetailEndAt>
    <PatternDetail><![CDATA[<div class="cxtLeft">.*?<span class="spanTime">(?<PubDate>.*?)</span>.*?<h2 class="Lead">(?<Teaser>.*?)</h2>.*?<div class="fck_detail">(?<ContentBody>.*?)</div>.*?</div>]]></PatternDetail>
    <LinkCommonPartern>
      <![CDATA[href="(?!javascript:)(?=[^"]*[A-Z])(?<LinkUrl>[^"<]+)?".*?>(?!<)(?<LinkText>[^"<]+)</a>]]>
    </LinkCommonPartern>
  </data>
